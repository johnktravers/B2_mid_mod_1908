<h1>All Students</h1>

<% @students.each do |student| %>
  <section id="student-<%= student.id %>">
    <h3><%= student.name %></h3>
    <p>Cohort: <%= student.cohort_num %></p>
    <p>Instructors:</p>
    <ul>
      <% student.instructors.each do |instructor| %>
        <li><%= link_to instructor.name, "/instructors/#{instructor.id}" %></li>
      <% end %>
    </ul>
  </section>
  <br>
<% end %>
